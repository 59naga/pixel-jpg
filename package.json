{
  "name": "pixel-jpg",
  "version": "0.0.0-alpha.2",
  "description": "Parse JPEG to ImageData in cross-platform",

  "files": [
    "index.js",
    "lib"
  ],
  "browser": {
    "./src": false
  },

  "scripts": {
    "start": "jasminetea --watch",
    "localhost": "zuul test/api.spec --local 59798 --open",

    "test": "jasminetea --cover --report --lint",

    "prepublish": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "coffee --bare --compile index.coffee",
    "postcompile": "coffee --output lib --bare --compile src",
    "postpublish": "zuul test/api.spec && echo skip || echo skip"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "dependencies": {
    "jpeg-js": "git://github.com/59naga/jpeg-js",
    "pixel-util": "0.0.0-beta.2"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^10.2.3",
    "coffeeify": "^1.1.0",
    "fixture-images": "^0.1.3",
    "ghooks": "^0.3.2",
    "jasminetea": "^0.2.1-alpha.2",
    "varify": "^0.1.1",
    "zuul": "^3.0.0"
  },

  "keywords": [
    "pixel",
    "jpg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/pixel-jgp.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/pixel-jgp/issues"
  },
  "homepage": "https://github.com/59naga/pixel-jgp#readme"
}
